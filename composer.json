{
  "name": "imbue/wefact-api-php",
  "description": "WeFact API client library for PHP.",
  "keywords": [
    "wefact",
    "php",
    "sdk",
    "api",
    "invoicing"
  ],
  "license": "MIT",
  "homepage": "https://github.com/imbue/wefact-api-php",
  "authors": [
    {
      "name": "Liam Boer",
      "email": "liamboer@outlook.com"
    }
  ],
  "require": {
    "php": "^7.2 || ^7.3 || ^8.0",
    "guzzlehttp/guzzle": "^6.3 || ^7.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 || ^6.5 || ^7.1 || ^9"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Imbue\\WeFact\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit tests"
  }
}
